<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chab.dog.dogoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.RegisterActivity"></activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.OrdenarActivity" />
        <activity android:name=".activities.HistorialActivity" />
        <activity android:name=".activities.MenuActivity" />
        <activity android:name=".activities.CuponesActivity" />
        <activity android:name=".activities.HomeActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>